    "@xmldom/xmldom": "^0.8.6",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-offline": "^6.0.0",
    "react-native-rss-parser": "^1.5.1",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-vector-icons": "^9.2.0",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.5",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
